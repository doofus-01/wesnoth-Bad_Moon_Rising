#: ./lua/equipment_list.lua:3365
msgid "Silver Arrows"
msgstr ""

#: ./lua/equipment_list.lua:3379
msgid "Starburst Arrows"
msgstr ""

#: ./lua/equipment_list.lua:3382
msgid "The soft metal of these arrowheads is impregnated with a shards of a very rare crystal, causing them to explode upon impact, with an otherworldly flame.  Grants a new arcane attack."
msgstr ""

#: ./lua/equipment_list.lua:3394
msgid "Small Dagger"
msgstr ""

#: ./lua/equipment_list.lua:3396 ./lua/equipment_list.lua:3410 ./lua/equipment_list.lua:3424 ./lua/equipment_list.lua:3442
msgid "Weapon for anyone to use"
msgstr ""

#: ./lua/equipment_list.lua:3397
