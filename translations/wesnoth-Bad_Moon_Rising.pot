#. [message]: speaker=Kashja
#: ./scenarios1/0_00_Prologue.cfg:467
msgid "Never!"
msgstr ""

#. [message]
#: ./scenarios1/0_00_Prologue.cfg:494
msgid "Die, vile thing!"
msgstr ""

#. [message]
#: ./scenarios1/0_00_Prologue.cfg:515
msgid "Come here, human!"
msgstr ""

#. [message]: speaker=Shaema
#: ./scenarios1/0_00_Prologue.cfg:535
msgid "No, Kashja!  Bring the Eye to our machine!"
